/* Requires the Docker Pipeline plugin */
pipeline {
    agent { 
        docker { 
                    image 'amazon/aws-cli'
                    args '--entrypoint='
               } 
        }
    stages {
        stage('build') {
            steps {
                    withAWS(region: 'us-east-2') {
                        sh 'aws --version'
                    }
            }
        }
    }
}
